<includes>
	<include name="HomeEspnSchedule">
			<control type="group">
				<posx>10</posx>
				<posy>10</posy>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>250</width>
					<height>600</height>
					<texture>floor.png</texture>
					<animation effect="fade" time="250" condition="Window.Previous(Home)">WindowOpen</animation>
					<animation effect="fade" time="250" condition="Window.Next(Home)">WindowClose</animation>
				</control>
				<control type="panel" id="999999">
				      <description>My first list container</description>
				      <posx>5</posx>
				      <posy>5</posy>
				      <width>250</width>
				      <height>600</height>
				      <visible>true</visible>
				      <onup>2</onup>
				      <ondown>3</ondown>
				      <onleft>1</onleft>
				      <onright>1</onright>
				      <viewtype label="3D list">wall</viewtype>
				      <orientation>vertical</orientation>
				      <pagecontrol>25</pagecontrol>
				      <autoscroll>true</autoscroll>
				      <scrolltime tween="sine" easing="out">200</scrolltime>
				      <itemlayout width="300" height="35">
						
						<control type="image">
							<posx>0</posx>
							<posy>0</posy>
							<width>30</width>
							<height>30</height>
							<aspectratio scalediffuse="true">keep</aspectratio>
							<info>ListItem.Icon</info>
						</control>
						<control type="label">
							<posx>32</posx>
							<posy>3</posy>
							<width>200</width>
							<height>10</height>
							<font>font8</font>
							<aligny>center</aligny>
							<selectedcolor>green</selectedcolor>
							<textcolor>white</textcolor>
							<shadowcolor>black</shadowcolor>
							<align>left</align>
							<info>ListItem.Label</info>
						</control>
						<control type="label">
							<posx>32</posx>
							<posy>10</posy>
							<width>200</width>
							<height>20</height>
							<font>font8</font>
							<aligny>center</aligny>
							<selectedcolor>green</selectedcolor>
							<textcolor>grey</textcolor>
							<shadowcolor>black</shadowcolor>
							<align>left</align>
							<info>ListItem.Label2</info>
						</control>
						<control type="image">
							<posx>232</posx>
							<posy>0</posy>
							<width>15</width>
							<height>15</height>
							<aspectratio scalediffuse="true">keep</aspectratio>
							<texture fallback="blank.png">ListItem.property(tv)</texture>
						
						</control>
				      </itemlayout>
				      <focusedlayout height="20" width="250">
						<control type="image">
							<posx>5</posx>
							<posy>3</posy>
							<width>22</width>
							<height>22</height>
							<info>ListItem.Icon</info>
						</control>
						<control type="label">
							<posx>0</posx>
							<posy>3</posy>
							<width>250</width>
							<height>10</height>
							<font>font8</font>
							<aligny>center</aligny>
							<selectedcolor>green</selectedcolor>
							<textcolor>white</textcolor>
							<shadowcolor>black</shadowcolor>
							<align>left</align>
							<info>ListItem.Label</info>
						</control>
						<control type="label">
							<posx>0</posx>
							<posy>0</posy>
							<width>300</width>
							<height>22</height>
							<font>font6</font>
							<aligny>center</aligny>
							<selectedcolor>green</selectedcolor>
							<textcolor>grey</textcolor>
							<shadowcolor>black</shadowcolor>
							<align>right</align>
							<info>ListItem.Label2</info>
						</control>
						<control type="image">
							<posx>0r</posx>
							<posy>0</posy>
							<width>30</width>
							<height>30</height>
							<aspectratio scalediffuse="true">keep</aspectratio>
							<info>ListItem.property(tv)</info>
						</control>
				      </focusedlayout>
				</control>
			
		</control>
	</include>
</includes>